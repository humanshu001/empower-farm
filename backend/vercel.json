{
    "version": 2,
    "builds": [
     {
      "src": "server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"] }
     },
     {
        "src": "./images/*",

        "use": "@vercel/static"
    }
    ],
    "routes": [
     {
      "src": "/(.*)",
      "dest": "server.js"
     }
    ]
   }